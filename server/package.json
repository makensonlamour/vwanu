{
  "name": "server",
  "version": "1.0.0",
  "description": "vwanu server",
  "main": "index.js",
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "scripts": {
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "dev": "nodemon",
    "test:local": "jest --watch --no-cache --detectOpenHandles",
    "test": "jest --coverage --passWithNoTests --detectOpenHandles --verbose",
    "build": "tsc",
    "preinstall": "npm i nodemon -g",
    "start": "NODE_ENV=development node src/bin/index.js",
    "start:staging": "NODE_ENV=staging node src/bin/index.js",
    "lint": "eslint src/**/*.ts ",
    "lint:fix": "eslint --fix src/**/*.ts ",
    "lint:prettier": "prettier  --fix src/**/*.js*",
    "test:watch": "jest --watch --detectOpenHandles",
    "devops": "tsnd --respawn --transpile-only --files --exit-child --watch src src/bin/index.ts"
  },
  "keywords": [
    "vwanu",
    "social",
    "network"
  ],
  "author": "WebVital",
  "license": "MIT",
